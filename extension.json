{
	"version": "0.0.1",
	"apiVersion": "1.0.0",
	"author": "ZOMLI",
	"description": "Measure the low hold time ( between the first low level to the last low level).It will show the sum/mean/max/min of low_hold time in measurement interval",
	"name": "low hold time Measurement",


	"extensions": {
		"high_hold measure": {
			"type": "DigitalMeasurement",
			"entryPoint": "LowHoldTimeMeasurement.LowHoldTimeMeasurement",
			"metrics": {
				"lowholdsum": { "name": "low hold Time sum", "notation": "T<sub>Lsum</sub>","units": "s" },
				"lowholdmean": { "name": "low hold Time mean", "notation": "T<sub>Lmean</sub>","units": "s" },
				"lowholdmax": { "name": "low hold Time max", "notation": "T<sub>Lmax</sub>","units": "s" },
				"lowholdmin": { "name": "low hold Time min", "notation": "T<sub>Lmin</sub>","units": "s" }
			}
		}
	}
}